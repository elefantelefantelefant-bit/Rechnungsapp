{
  "expo": {
    "name": "Truthahn-App",
    "slug": "rechnungsapp",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "scheme": "rechnungsapp",
